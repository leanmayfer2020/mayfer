<section class="bgwhite p-t-20 p-b-60">
		<div class="container">
			<div class="row">
				<div class="row globoContacto">
					
					<div class="table globoContactoContent" style="margin-bottom:50px;">
						<h3 style="text-align: center;width: 100%;margin-bottom: 10px;">{{config.contact_data.title}}</h3>
						<p>
							{{config.contact_data.leyend}}
						</p>
						<div class="p-r-20 p-r-0-lg">
							<iframe src="{{config.contact_data.location}}" width="100%" height="503" frameborder="0" style="border:0" allowfullscreen></iframe>
							<div id="capa"></div>
						</div>
					</div>

					{{#if_eq brand "mayfer"}}
						<div class="table globoContactoContent" style="margin-bottom:50px;">
							<h3 style="text-align: center;width: 100%;margin-bottom: 10px;">{{config.contact_data.newStore.title}}</h3>
							<p>
								{{config.contact_data.newStore.leyend}}
							</p>
							<div class="p-r-20 p-r-0-lg">
								<iframe src="{{config.contact_data.newStore.location}}" width="100%" height="503" frameborder="0" style="border:0" allowfullscreen></iframe>
								<div id="capa"></div>
							</div>
						</div>
					{{/if_eq}}

				</div>

			</div>
		</div>
	</section>